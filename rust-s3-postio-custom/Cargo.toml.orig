[package]
name = "rust-s3"
version = "0.5.1"
authors = ["Drazen Urch", "Nick Stevens"]
description = "Tiny Rust library for working with Amazon S3."
repository = "https://github.com/durch/rust-s3"
readme = "README.md"
keywords = ["Amazon", "AWS", "S3"]
license = "MIT"
documentation = "https://durch.github.io/rust-s3/"

[lib]
name = "s3"
path = "src/lib.rs"

[dependencies]
chrono = "0.2.0"
curl = "0.4.0"
error-chain = "0.10"
hex = "0.2.0"
hmac = "0.1"
serde_derive = "0.9"
serde = "0.9"
serde-xml-rs = "0.1.2"
#serde_xml = "0.9"
sha2 = "0.5"
url = "1.2.4"

